{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "version": "0.8.7",
  "minimum_chrome_version": "22",
  "browser_action": {
    "default_icon": {
      "19": "media/images/browser_action/cbutton19.png",
      "38": "media/images/browser_action/cbutton38.png"
    },
    "default_title": "cottonTracks"
  },
  "icons": {
    "16": "/media/images/manifest/cotton_tracks_favicon_16.png",
    "48": "/media/images/manifest/cotton_tracks_favicon_48.png",
    "128": "/media/images/manifest/cotton_tracks_favicon_128.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "background": {
    "page": "background.html"
  },
  "permissions": [ "contextMenus", "management", "tabs", "storage", "https://github.com/*" ],
  "content_security_policy": "default-src 'self'; script-src 'self' http://localhost:3000 https://localhost:3000 'unsafe-eval'; connect-src http://localhost:3000 https://localhost:3000; style-src * 'unsafe-inline' 'self' blob:; img-src 'self' data:;"
}
